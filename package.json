{
  "name": "progettoplugin",
  "displayName": "ProgettoPlugin",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "extension-views",
          "title": "Extension Views",
          "icon": "./resources/icon.png"
        }
      ]
    },
    "views": {
      "extension-views": [
        {
          "type": "webview",
          "id": "extension.loginView",
          "name": "Login"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.login",
        "title": "Login",
        "category": "US EXT"
      },
      {
        "command": "extension.generateTest",
        "title": "Generate Unit Test",
        "category": "US EXT"
      },
      {
        "command": "extension.runTests",
        "title": "Run Test for Workspace",
        "category": "US EXT"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "mock-fs": "^5.2.0",
    "progettolib": "file:../ProgettoLib",
    "sinon": "^17.0.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vscode": "^1.1.37",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
